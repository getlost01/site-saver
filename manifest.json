{
  "manifest_version":3,
  "version":"1.1.11.7",
  "name":"Site Saver",
  "author": "Aagam Jain",    
  "description": "This extension is useful to save your online sites/current tabs links in just one click.",         
  "action":{
      "default_popup":"index.html",
      "default_icon":"logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "homepage_url": "https://github.com/getlost01/site-saver",
  "icons": {
    "48": "logo.png",
    "98": "logo.png"
  },
  "permissions":[
  "tabs"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+8",
        "mac": "Command+Shift+8"
      }
    }
  }
}
