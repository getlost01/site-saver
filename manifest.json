{
    "manifest_version":3,
    "version":"1.1.10.1",
    "name":"Site Tracker",
    "author": "Aagam Jain",    
    "description": "This extension is useful to save your online sites/current tabs links in just one click.",         
    "action":{
        "default_popup":"index.html",
        "default_icon":"logo.png"
    },
    	
    "homepage_url": "https://github.com/getlost01/site-saver-extension.github.io",
    "icons": {
      "48": "logo.png",
      "98": "logo.png"
    },
	  "permissions":[
	  "tabs"
	  ]
}
